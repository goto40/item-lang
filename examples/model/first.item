package basic_stuff {
    property_set props {
        property optional mydescription: STRING
    }
}

package first (property_set basic_stuff.props) {
    rawtype my_int UINT 32

    struct First {
        scalar x: my_int
        scalar y: my_int
    }
}

package first_other {
    struct First {
        scalar x: first.my_int
        scalar y: first.my_int
    }
}
